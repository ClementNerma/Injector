{
    "manifest_version": 2,
    "name": "Injector",
    "author": "Cl√©ment Nerma",
    "version": "1.8.1",
    "description": "Inject JavaScript and CSS resources on any website",
    "icons": {
        "16": "icons/16x16.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/16x16.png",
            "48": "icons/48x48.png",
            "128": "icons/128x128.png"
        },

        "default_title": "Injector",
        "default_popup": "src/popup/index.html"
    },

    "background": {
        "page": "src/background/index.html"
    },

    "permissions": ["activeTab", "tabs", "storage", "http://*/", "https://*/"]
}
